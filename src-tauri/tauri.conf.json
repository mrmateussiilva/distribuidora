{
  "productName": "PDV Distribuidora",
  "version": "1.0.0",
  "identifier": "com.distribbuidora.pdv",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1422"
  },
  "app": {
    "windows": [
      {
        "title": "PDV - Distribuidora",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/256x256@2x.png",
      "icons/512x512.png",
      "icons/512x512@2x.png"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
